RewriteEngine On
RewriteCond %{HTTP_HOST} instahotstar\.com [NC]
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://instahotstar.com/$1 [R,L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php/$1 [L]


